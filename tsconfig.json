{
  "files": [],
  "references": [
    { "path": "./tsconfig.app.json" },
    { "path": "./tsconfig.node.json" }
  ],
   
  "compilerOptions": {
    "lib": ["dom", "esnext"],
    "jsx" : "react-jsx",
    "module":"esnext",
    "target": "es6",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipDefaultLibCheck": true,

    "baseUrl": ".",
    "paths": {
      "@/*": ["./src"]
     
    },
    "noImplicitAny": false,
    "noUnusedParameters": false,
    "skipLibCheck": true,
    "allowJs": true,
    "noUnusedLocals": false,
    "strictNullChecks": false
  },
}

